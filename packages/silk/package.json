{
  "name": "@loveui/silk",
  "description": "Animated silk background built with React Three Fiber and Three.js.",
  "version": "0.1.1",
  "type": "module",
  "license": "AGPL-3.0-or-later",
  "keywords": [
    "react",
    "three",
    "background",
    "animation",
    "loveui"
  ],
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
