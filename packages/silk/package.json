      "optional": true
    }
  },
  "devDependencies": {
    "@react-three/fiber": "^9.4.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.1",
    "@types/three": "^0.170.0",
    "@workspace/typescript-config": "workspace:*",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "three": "^0.181.0",
